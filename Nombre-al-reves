package com.cmc.Alejandrotaller;

public class Ejercicio {
	
	static String nombre = "BarackObama";
	
	
	public static String invertirCadena() {
		
		String sCadenaInvertida = "";
		for (int x=nombre.length()-1;x>=0;x--) {
			sCadenaInvertida = sCadenaInvertida + nombre.charAt(x);
		}
		
		return sCadenaInvertida;
	}
	
	public static void main(String args []) {
		
		System.out.println(invertirCadena());		
	}
	
}
